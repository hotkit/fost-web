{

    "webserver": {
        "load": ["fost-webserver-test-gd.dll"],

        "hosts": {
            "localhost:8001": "testserver.template"
        },

        "views": {
            "testserver.template": {
                "view": "fost.middleware.template",
                "configuration": {
                    "template": "Templates/testserver/skin.html",
                    "view": "testserver"
                }
            },
            "testserver": {
                "view": "fost.view.pathprefix",
                "configuration": {
                    "": "testserver.static",
                    "test/": "fost.test.runner"
                }
            },
            "testserver.static": {
                "view": "fost.static",
                "configuration": {
                    "root": "Static/testserver"
                }
            }
        }
    }

}

